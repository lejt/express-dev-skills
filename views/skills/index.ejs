<%- include('../partials/header') %>

<body>
    <h1>My Dev Skills</h1>
    <ul>
      <% skills.forEach(function(skill) { %>
        <li>
          <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
            - 
          proficiency: <%= skill.proficiency %>
        </li>
      <% }); %>
    </ul>
</body>


